@model testweb2.Models.User

@{
    ViewBag.Title = "Login";
}

<form class="form-signin" role="form" method="post" action="Login">
    @Html.AntiForgeryToken()

    <h2 class="text-center">로그인</h2>

    <div class="form-group" id="divId">
        <input type="text" class="form-control onlyAlphabetAndNumber" id="UserId" name="UserId" data-rule-required="true" placeholder="30자이내의 알파벳, 언더스코어(_), 숫자만 입력 가능합니다." maxlength="30">
    </div>
    <div class="form-group" id="divPassword">
        <input type="password" class="form-control" id="UserPassword" name="UserPassword" data-rule-required="true" placeholder="패스워드" maxlength="30">
    </div>

    <div class="form-group text-center" id="divSubmit">
        <input type="submit" name="btnSubmit" value="로그인" class="btn btn-primary justify-content-center">
    </div>
</form>




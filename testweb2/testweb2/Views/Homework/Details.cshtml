@model MvcMovie.Models.Homework

@{
    ViewBag.Title = "Details";
}

<div>
    <p>
        <h2>@Model.Title</h2>
    </p>
    <span>@Model.T_Name | </span><span>@Model.Subject | </span><span>@Model.Date | </span>
</div>
<hr />
<div class="div-contents">
    @Html.Raw(Model.Contents)
</div>
<p>
    <a class="btn btn-primary" href="@Model.NoteNo">제거</a>
    @Html.ActionLink("Edit", "Edit", new { id = Model.NoteNo }) |
    @Html.ActionLink("Back to List", "Index")
</p>

@Html.Action("comment","Comments",new { NoteNo = Model.NoteNo })

